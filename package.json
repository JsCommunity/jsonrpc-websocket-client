{
  "name": "jsonrpc-websocket-client",
  "version": "0.0.0",
  "license": "ISC",
  "description": "JSON-RPC 2 over WebSocket",
  "keywords": [
    "json",
    "json-rpc",
    "jsonrpc",
    "jsonrpc2",
    "rpc",
    "websocket"
  ],
  "homepage": "https://github.com/julien-f/jsonrpc-websocket-client",
  "bugs": "https://github.com/julien-f/jsonrpc-websocket-client/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/julien-f/jsonrpc-websocket-client.git"
  },
  "author": {
    "name": "Julien Fontanet",
    "email": "julien.fontanet@isonoe.net"
  },
  "preferGlobal": false,
  "main": "dist/",
  "bin": {},
  "files": [
    "dist/"
  ],
  "dependencies": {
    "babel-runtime": "^5.4.7",
    "event-to-promise": "^0.4.0",
    "json-rpc-peer": "^0.11.0",
    "lodash.isarray": "^3.0.3",
    "lodash.isstring": "^3.0.1",
    "lodash.startswith": "^3.0.1",
    "make-error": "^1.0.2",
    "ws": "^0.8.0"
  },
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-eslint": "^4.1.3",
    "mocha": "^2.2.5",
    "must": "^0.13.1",
    "source-map-support": "^0.3.1",
    "standard": "^5.3.1"
  },
  "scripts": {
    "build": "babel --optional=runtime --compact=true --source-maps --out-dir=dist/ src/",
    "dev": "babel --watch --optional=runtime --source-maps --out-dir=dist/ src/",
    "lint": "standard",
    "posttest": "npm run lint",
    "prepublish": "npm run build",
    "test": "mocha --require source-map-support/register 'dist/**/*.spec.js'",
    "test-dev": "mocha --require source-map-support/register --watch --reporter=min 'dist/**/*.spec.js'"
  },
  "standard": {
    "ignore": [
      "dist/**"
    ],
    "parser": "babel-eslint"
  }
}
